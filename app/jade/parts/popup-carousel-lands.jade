.popup-wrapper.mfp-hide#popupCarouselLands
	.popup
		.popup-close: i.fa.fa-close
		.section-title.section-title_popup Kleine Wohnung Nordseite
		.connected-carousels
			.stage
				.carousel-popup.carousel-stage
					ul
						each img in popupImg.srcOriginal
							li: img.carousel-popup__img(src="img/popup-carousel/#{img}", alt="#{img}")
				aside.stage-aside
					ul.stage-aside__list
						li
							h4.stage-aside__title Kontact
						li.stage-aside__item
							i.fa.fa-envelope.fa-lg
							a.stage-aside__item.mailto(href="mailto:info@alte-ziegelei.de")!= 'info@alte-ziegelei.de'
						li.stage-aside__item!= '<i class="fa fa-phone fa-lg stage-aside__ico"></i>03551/947324'
						li.stage-aside__item.stage-aside__item_address!= '<i class="fa fa-map-marker fa-2x"></i>An der Ziegelei 5, 02763 Zittau / OT Hartau'
				.carousel-popup__arrows
					a.prev.prev-stage(href=""): i.fa.fa-angle-left
					a.next.next-stage(href=""): i.fa.fa-angle-right
			.navigation
				a.prev.prev-navigation(href=""): i.fa.fa-angle-left.fa-4x
				a.next.next-navigation(href=""): i.fa.fa-angle-right.fa-4x
				.carousel-popup.carousel-navigation
					ul
						each thumb,index in popupImg.srcThumb
							if index == 0
								li.active(data-id='#{index}')
									img(src="img/popup-carousel/#{thumb}", alt="#{thumb}")
							else
								li(data-id='#{index}')
									img(src="img/popup-carousel/#{thumb}", alt="#{thumb}")
			.description-block__wrapper
				.description-block
					ul.description-stage
						each roomDescription,index in roomContent.roomDescriptions
							if index == 0
								li.section-text.active.animated.fadeIn= roomDescription
							else
								li.section-text= roomDescription